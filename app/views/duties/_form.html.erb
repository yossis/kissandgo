<%= form_for(duty) do |f| %>
  <% if duty.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(duty.errors.count, "error") %> prohibited this duty from being saved:</h2>

      <ul>
      <% duty.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :class_room_id %>
    <%= f.number_field :class_room_id %>
  </div>

  <div class="field">
    <%= f.label :start_date %>
    <%= f.date_select :start_date %>
  </div>

  <div class="field">
    <%= f.label :end_date %>
    <%= f.date_select :end_date %>
  </div>

  <div class="field">
    <%= f.label :parents_in_duty %>
    <%= f.number_field :parents_in_duty %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.number_field :user_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
